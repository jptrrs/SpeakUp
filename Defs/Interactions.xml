<!-- ============ TRAITS ============ -->
	<InteractionDef ParentName="SpeakUpReply"><!-- ascetic — wisdom -->
		<defName>AsceticWisdom</defName>
		<label>ascetic — wisdom</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->...</li>
				<!--Good opinion of recipient-->

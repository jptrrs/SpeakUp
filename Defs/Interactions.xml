<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<InteractionDef Name="SpeakUpReply" Abstract="True">
		<symbol>Things/Mote/SpeechSymbols/Speech</symbol>
		<initiatorXpGainSkill>Social</initiatorXpGainSkill>
		<initiatorXpGainAmount>2</initiatorXpGainAmount>
	</InteractionDef>

<!-- ============ GENERIC CHITCHAT ============ -->
	<InteractionDef ParentName="SpeakUpReply">
		<defName>ChitChat_generic</defName>
		<label>Chitchat generic</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0,tag=ChitChat_generic2)->Yes, [RECIPIENT_nameDef]?</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
	<InteractionDef ParentName="SpeakUpReply">
		<defName>ChitChat_generic2</defName>
		<label>Chitchat generic2</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->Nothing — forget it.</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->Mmm... it's nothing — sorry to have bothered you.</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>

<!-- ==JOBS== -->
	<InteractionDef ParentName="SpeakUpReply"><!--Cleaning-->
		<defName>Cleaning</defName>
		<label>cleaning</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
					<li>good_opinion(INITIATOR_thoughtLabel==hideous environment,priority=1)->Thanks, [RECIPIENT_nameDef] — this place looks [ugly], [cleaning] it certainly helps!</li>
					<li>good_opinion(INITIATOR_thoughtLabel==ugly environment,priority=1)->Thanks, [RECIPIENT_nameDef] — this place looks [unsightly], [cleaning] it helps for sure!</li>
					<li>good_opinion->Thanks, [RECIPIENT_nameDef] — this place certainly needs some [cleaning].</li>
					<li>good_opinion(INITIATOR_thoughtLabel==pretty environment,priority=1)->Thanks, [RECIPIENT_nameDef]. This place is so nice — let's keep it [clean]!</li>
					<li>good_opinion(INITIATOR_thoughtLabel==beautiful environment,priority=1)->Thanks, [RECIPIENT_nameDef]. This place is so beautiful — let's keep it [clean]!</li>
					<li>good_opinion(INITIATOR_thoughtLabel==gorgeous environment,priority=1)->Thanks, [RECIPIENT_nameDef]. This place is so gorgeous — let's keep it [clean]!</li>
				<!--WILDCARDS-->
				<!--clean-->
				<li>clean->clean</li>
				<li>clean->tidy</li>
				<li>clean->spotless</li>							
				<!--cleaning (action) -->
				<li>cleaning->cleaning</li>
				<li>cleaning->scrubbing</li>
				<li>cleaning->washing</li>
				<!--ugly-->
				<li>ugly->ugly</li>
				<li>ugly->hideous</li>
				<li>ugly->horrible</li>
				<li>ugly->frightful</li>
				<li>ugly->grisly</li>
				<li>ugly->gruesome</li>
				<li>ugly->grim</li>
				<li>ugly(INITIATOR_intellectual_level>=5)->repugnant</li>
				<li>ugly(INITIATOR_intellectual_level>=5)->vile</li>
				<li>ugly(INITIATOR_intellectual_level>=5)->grotesque</li>
				<!--unsightly-->
				<li>unsightly->unsightly</li>
				<li>unsightly->messy</li>
				<li>unsightly->shabby</li>
				<li>unsightly->sloppy</li>				
				<li>unsightly->trashy</li>
				<li>unsightly->trashed</li>
				<li>unsightly->unkempt</li>
				<li>unsightly(INITIATOR_intellectual_level>=5)->unappealing</li>
				<li>unsightly(INITIATOR_intellectual_level>=5)->unattractive</li>				
				<li>unsightly(INITIATOR_intellectual_level>=10)->bedraggled</li>				
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>	
	<InteractionDef ParentName="SpeakUpReply">
		<defName>CloudWatching</defName>
		<label>cloudwatching</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Idle-->
				<li>r_logentry(INITIATOR_jobDefName==GotoWander,priority=1)->[idle]</li>
				<li>r_logentry(INITIATOR_jobDefName==Ingest,priority=1)->[idle]</li>
				<li>r_logentry(INITIATOR_jobDefName==Wait_Wander,priority=1)->[idle]</li>
				<li>r_logentry(INITIATOR_jobDefName==Skygaze,priority=1)->[idle]</li>				
				<li>r_logentry(INITIATOR_jobDefName==SocialRelax,priority=1)->[idle]</li>
				<li>r_logentry(INITIATOR_jobDefName==Sow,priority=1)->[idle]</li>
				<li>r_logentry(INITIATOR_jobDefName==StandAndBeSociallyActive,priority=1)->[idle]</li>
					<!--Bad opinion of recipient-->
					<li>idle(INITIATOR_opinion&lt;0)->No it doesn't, [RECIPIENT_nameDef].</li>
					<li>idle(INITIATOR_opinion&lt;0)->It actually reminds me more of the way I'd rather be right now — far from annoying people like you.</li>
					<!--Good opinion of recipient-->
					<li>idle(INITIATOR_opinion>=0)->Yes... wow, you're right — how funny!</li>
					<li>idle(INITIATOR_opinion>=0)->Mmm... you might be right — but I'd rather say it looks like [a_cloudshape].</li>
						<li>a_cloudshape->a [Animal]</li>
						<li>a_cloudshape->a [Apparel]</li>
						<li>a_cloudshape->a [TreeType]</li>
						<li>a_cloudshape->a [Vegetable]</li>
						<li>a_cloudshape->a [Weapon]</li>
				<!--Busy-->
				<li>r_logentry->[busy]</li>
					<!--Bad opinion of recipient-->
					<li>busy(INITIATOR_opinion&lt;0)->...</li>
					<!--Good opinion of recipient-->
					<li>busy(INITIATOR_opinion>=0)->Sorry, [RECIPIENT_nameDef] — I'm busy right now.</li>
					<li>busy(INITIATOR_opinion>=0)->Uh? Ah... well.. sorry I missed it, [RECIPIENT_nameDef].</li>					
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
			

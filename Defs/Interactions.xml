	<InteractionDef ParentName="SpeakUpReply"><!--shooting - hit-->
		<defName>ShootingHit</defName>
		<label>shooting — hit</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->[bad_opinion]</li>
				<li>bad_opinion->You just got lucky!</li>
				<li>bad_opinion->Lucky...</li>
				<li>bad_opinion->Small victories, big failures...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
				<li>good_opinion->Great job, [RECIPIENT_nameDef]!</li>
				<li>good_opinion->Good shot, [RECIPIENT_nameDef]!</li>
				<li>good_opinion->Deadshot, [RECIPIENT_nameDef]!</li>
				<li>good_opinion->Wow, [RECIPIENT_nameDef] — good aim!</li>
				<li>good_opinion(RECIPIENT_gender==Male,INITIATOR_intellectual_level&lt;5)->Atta boy!</li>
				<li>good_opinion(RECIPIENT_gender==Female,INITIATOR_intellectual_level&lt;5)->Atta girl!</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
	<InteractionDef ParentName="SpeakUpReply"><!--shooting - miss-->
		<defName>ShootingMiss</defName>
		<label>shooting — miss</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->[bad_opinion]</li>
				<li>bad_opinion->...</li>
				<li>bad_opinion->Lame...</li>
				<li>bad_opinion->Pathetic...</li>
				<li>bad_opinion->You really do suck at that, huh.</li>
				<li>bad_opinion->Seems like you can't do anything right, [RECIPIENT_nameDef].</li>
				<li>bad_opinion->So close — yet so far...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
				<li>good_opinion->Try again, [RECIPIENT_nameDef]!</li>
				<li>good_opinion->Oh, aim a little more to the [left_right] next time.</li>
				<li>good_opinion->Maybe try from a different angle...</li>
				<li>good_opinion->Close one!</li>
				<li>good_opinion->Missed by a hair!</li>
				<li>left_right->left</li>
				<li>left_right->right</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>		
	

<!-- ============ MOOD ============ -->
	<InteractionDef ParentName="SpeakUpReply"><!-- Calm -->
		<defName>Calm</defName>
		<label>react to — calm</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->[bad_opinion]</li>
				<li>bad_opinion(INITIATOR_mood&lt;0.4)->[stressed]</li>
				<li>bad_opinion(INITIATOR_mood&lt;0.4,INITIATOR_trait==jealous,priority=1)->[stressed]</li>
				<li>bad_opinion->...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
				<li>good_opinion(INITIATOR_mood&lt;0.4,priority=1)->...</li>
				<li>good_opinion->[calm]</li>
				
				<!--WILDCARDS-->
				<li>stressed(INITIATOR_mood>0.3)->*frowns*</li>
				<li>stressed->*scowls*</li>
				<li>stressed->*glares*</li>
				<li>stressed->*glowers*</li>
				<li>stressed(INITIATOR_mood&lt;=0.2)->*mutters angrily*</li>
				
				<li>calm->*smiles*</li>
				<li>calm(INITIATOR_mood>=0.6)->*beams a smile to [RECIPIENT_nameDef]*</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
	
	<InteractionDef ParentName="SpeakUpReply"><!-- Stressed -->
		<defName>Stressed</defName>
		<label>react to — stressed</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->[bad_opinion]</li>
				<li>bad_opinion(INITIATOR_mood&lt;0.4,priority=1)->*scowls back*</li>
				<li>bad_opinion->...</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
				<li>good_opinion(INITIATOR_mood&lt;0.4,priority=1)->...</li>
				<li>good_opinion->*smiles back*</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>	

	<InteractionDef ParentName="SpeakUpReply"><!-- Singing -->
		<defName>Singing</defName>
		<label>react to — singing</label>
		<logRulesInitiator>
			<rulesStrings>
				<!--Bad opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion&lt;0)->[bad_opinion]</li>
				<li>bad_opinion->...</li>
				<li>bad_opinion(INITIATOR_trait==kind,priority=1)->[good_opinion]</li>
				<li>bad_opinion(RECIPIENT_trait==annoying voice)->Bravo, [RECIPIENT_nameDef] — that was a perfect impression of a squealing pig!</li>
				<li>bad_opinion(RECIPIENT_artistic_level&lt;5)->Stop that, [RECIPIENT_nameDef] — my ears are bleeding already!</li>
				<!--Good opinion of recipient-->
				<li>r_logentry(INITIATOR_opinion>=0)->[good_opinion]</li>
				<li>good_opinion(RECIPIENT_trait==annoying voice)->Err, [RECIPIENT_nameDef]... have you considered playing an instrument instead?</li>
				<li>good_opinion(RECIPIENT_artistic_level&lt;5)->[unskilled]</li>
					<li>unskilled->...</li>
					<li>unskilled(INITIATOR_artistic_level>=5)->Mmm, not bad [RECIPIENT_nameDef] — but you definitely need some singing lessons...</li>
				<li>good_opinion(RECIPIENT_artistic_level>=5)->[calm]</li>
					<li>calm->*smiles*</li>
					<li>calm(INITIATOR_mood>=0.6)->*beams a smile to [RECIPIENT_nameDef]*</li>
				<li>good_opinion(RECIPIENT_artistic_level>=10)->Bravo, [RECIPIENT_nameDef] — what a beautiful voice!</li>
				<li>good_opinion(RECIPIENT_artistic_level>=10)->Your voice is so pleasant to hear, [RECIPIENT_nameDef]!</li>
				<li>good_opinion(RECIPIENT_artistic_level>=15)->That was some amazing singing, [RECIPIENT_nameDef] — I loved it!</li>
				<li>good_opinion(RECIPIENT_artistic_level>=15)->Ahh... I just love hearing your voice, [RECIPIENT_nameDef]!</li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
	
	<InteractionDef ParentName="SpeakUpReply"><!-- How are you -->
		<defName>HowAreYou</defName>
		<label>how are you</label>
		<logRulesInitiator>
			<rulesStrings>
				<!-- bad opinion -->
				<li>r_logentry(INITIATOR_opinion&lt;-5)->[bad_opinion]</li>
				<li>bad_opinion->Leave me alone, [RECIPIENT_nameDef]!</li>
				<li>bad_opinion->Mind your business, [RECIPIENT_nameDef]!</li>
				<li>bad_opinion->None of your business, [RECIPIENT_nameDef]!</li>
				<li>bad_opinion->...</li>
				<!-- decent or good opinion, but sick -->
				<li>r_logentry(INITIATOR_opinion>=-5,INITIATOR_thoughtDefName==sick,priority=1)->[good_opinion_sick]</li>
				<li>good_opinion_sick(INITIATOR_mood>=0.7)->I'm not feeling well, but I'm sure I'll recover soon! [whataboutyou]</li>
				<li>good_opinion_sick(INITIATOR_mood>=0.5,INITIATOR_mood&lt;0.7)->I'm not feeling well, but I believe I'll recover soon. [whataboutyou]</li>
				<li>good_opinion_sick(INITIATOR_mood>=0.3,INITIATOR_mood&lt;0.5)->I'm not feeling well.</li>
				<li>good_opinion_sick(INITIATOR_mood&lt;0.3)->[furious] And on top of that, I'm sick!</li>				
				<!-- decent or good opinion -->
				<li>r_logentry(INITIATOR_opinion>=-5)->[good_opinion]</li>
				<li>good_opinion(INITIATOR_mood>=0.7)->I'm feeling [great], thank you! [whataboutyou]</li>
				<li>good_opinion(INITIATOR_mood>=0.5,INITIATOR_mood&lt;0.7)->I'm [fine], thanks. [whataboutyou]</li>
				<li>good_opinion(INITIATOR_mood>=0.3,INITIATOR_mood&lt;0.5)->[not_so_well]</li>
				<li>good_opinion(INITIATOR_mood&lt;0.3)->[furious]</li>
				<!-- WILDCARDS -->
				<li>great->great</li>
				<li>great->awesome</li>
				<li>great->groovy</li>
				<li>great->very well</li>
				<li>fine->fine</li>
				<li>fine->OK</li>
				<li>fine->good</li>
				<li>fine->doing fine</li>
				<li>fine->doing well</li>
				<li>not_so_well->Could be better...</li>
				<li>not_so_well->Not so well...</li>
				<li>not_so_well->So-so...</li>
				<li>not_so_well->Not too bad...</li>
				<li>not_so_well->I'm... OK, I guess.</li>
				<li>not_so_well->Same old, same old...</li>
				<li>furious->I hate my life!</li>
				<li>furious->I'm sick and tired of my life!</li>
				<li>furious->I feel terrible!</li>
				<li>furious->I'm losing it!</li>
				<li>furious->I'm outraged!</li>
				<li>furious->I'm pissed off!</li>
				<li>furious(INITIATOR_intellectual_level>=5)->I'm seething!</li>
				<li>furious(INITIATOR_intellectual_level>=5)->I'm wrathful!</li>
				<li>furious(INITIATOR_intellectual_level>=10)->I'm beside myself!</li>
				<li>furious(INITIATOR_intellectual_level>=10)->I'm irate!</li>
				<li>furious(INITIATOR_intellectual_level>=10)->I'm aggrieved!</li>
				<li>furious(INITIATOR_intellectual_level>=15)->I'm in high dudgeon!</li>
				<li>furious(INITIATOR_trait==abrasive)->I'm feeling like shit, OK?!</li>
				<li>furious(INITIATOR_trait==abrasive)->I just fucking hate my shitty life!</li>
				<li>furious(INITIATOR_trait==bloodlust)->Stay away from me — or I might punch you in the face!</li>
				<li>furious(INITIATOR_trait==brawler)->I'm so pissed off! I need something to smash!</li>
				<li>furious(INITIATOR_trait==pyromaniac)->If my anger could burn things around me, this whole area would turn to a desert wasteland.</li>
				<li>furious(INITIATOR_trait==tortured artist)->Nobody will ever understand the rough maelstrom swirling inside my tortured soul!</li>
				<!-- FOLLOWUP -->
				<li>whataboutyou(INITIATOR_opinion>=5,tag=HowAreYou2)->What about you?</li>
				<li>whataboutyou(INITIATOR_opinion&lt;5)-></li>
			</rulesStrings>
		</logRulesInitiator>
	</InteractionDef>
